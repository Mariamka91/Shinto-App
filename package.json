  "bugs": {
    "url": "https://github.com/Mariamka91/Shinto-App/issues"
  },
  "homepage": "https://github.com/Mariamka91/Shinto-App#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2"
  }
}
